{
	"name": "jsmm",
	"displayName": "jsmm",
	"description": "jsmm support",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.39.0"
	},
	"author": {"name": "Boris Kazakov", "email": "boris.kazakov@gmail.com"},
	"publisher": "kboris",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.jsmmEdit",
		"onLanguage:json"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.jsmmEdit",
				"title": "Edit jsmm",
				"icon" :{"dark": "icons/icon1.png", "light": "icons/icon1.png"}
			}
		],
		"jsonValidation": [
			{
				"fileMatch": "*.jsmm",
				"url": "./schemas/jsmm.json"
			}
		],
		"languages": [
			{
				"id": "json",
				"extensions": [
					".jsmm",
					".json"
				]
			}
		],
		"menus": {
			"editor/title": [
				{
					"when": "resourceExtname == .jsmm",
					"command": "extension.jsmmEdit",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}